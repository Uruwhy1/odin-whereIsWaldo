generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id    Int    @id @default(autoincrement())
  name  String
  minX  Float
  maxX  Float
  minY  Float
  maxY  Float
}


model Game {
  id           Int       @id @default(autoincrement())
  username     String
  startDate    DateTime
  endDate      DateTime?
  foundCounter Int       @default(0)
}
